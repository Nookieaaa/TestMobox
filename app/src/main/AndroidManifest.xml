<?xml version="1.0" encoding="utf-8"?>
<manifest package="nookdev.com.moboxtest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.nookdev.testmobox.view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.nookdev.testmobox.view.activity.DetailsActivity">
        </activity>

        <service
            android:name="com.nookdev.testmobox.model.TimerService"
            android:enabled="true"
            android:exported="false">
        </service>
    </application>

</manifest>